<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="quant_origin_stock_quant_tree_view">
            <field name="name">quant.origin.stock.quant.tree.view</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree"/>
            <field name="priority" eval="16"/>
            <field name="arch" type="xml">
                <field name="product_id" position="after">
                    <field name="origin"/>
                </field>
            </field>
        </record>

        <record model="ir.ui.view" id="quant_origin_stock_quant_form_view">
            <field name="name">quant.origin.stock.quant.form.view</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_form"/>
            <field name="priority" eval="16"/>
            <field name="arch" type="xml">
                <field name="product_id" position="after">
                    <field name="origin"/>
                </field>
            </field>
        </record>

    </data>
</openerp>