<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="generate_tracking_labels_colissimo_config_view">
            <field name="name">generate.tracking.labels.colissimo.config.view</field>
            <field name="model">base.config.settings</field>
            <field name="inherit_id" ref="base_setup.view_general_configuration"/>
            <field name="priority" eval="16"/>
            <field name="arch" type="xml">
                <form position="inside">
                    <separator string="Génération des étiquettes d'envoi Chronopost"/>
                    <group>
                        <field name="login_chronopost"/>
                        <field name="password_chronopost" password="1"/>
                        <field name="pre_alert_chronopost"/>
                    </group>
                </form>
            </field>
        </record>

    </data>
</openerp>