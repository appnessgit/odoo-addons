<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Cron to update synchronize Odoo and Outlook First Odoo -> Outlook for the deletion, then Outlook -> Odoo -->
    <record id="ouestacro_odoo_outlook_synchronisation_cron" model="ir.cron">
        <field name="name">Synchronisation Odoo-Outlook</field>
        <field name="model_id" ref="base.model_res_users"/>
        <field name="active" eval="True" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="nextcall">2019-11-01 01:00:00</field>
        <field name="numbercall">-1</field>
        <field name="doall">1</field>
        <field name="state">code</field>
        <field name="code">model.cron_launch_outlook_synchronisation()</field>
    </record>

</odoo>