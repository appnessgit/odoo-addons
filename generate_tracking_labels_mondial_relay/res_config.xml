<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="generate_tracking_labels_mondial_config_view">
            <field name="name">generate.tracking.labels.mondial.config.view</field>
            <field name="model">base.config.settings</field>
            <field name="inherit_id" ref="base_setup.view_general_configuration"/>
            <field name="priority" eval="16"/>
            <field name="arch" type="xml">
                <form position="inside">
                    <separator string="Génération des étiquettes d'envoi Mondial Relais"/>
                    <group>
                        <field name="login_mondial"/>
                        <field name="password_mondial" password="1"/>
                        <field name="societe_mondial"/>
                        <field name="marque_mondial"/>
                        <field name="code_marque_mondial"/>
                        <field name="origine_mondial"/>
                    </group>
                </form>
            </field>
        </record>

    </data>
</openerp>