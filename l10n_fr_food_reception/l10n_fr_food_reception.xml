<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="110n_fr_food_reception_picking_form">
            <field name="name">110n_fr_food_reception_picking_form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="priority" eval="16"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='pack_operation_product_ids']/tree//field[@name='product_id']" position="after">
                    <field name="maximal_core_temperature"/>
                    <field name="refusal_temperature"/>
                    <field name="reception_temperature"/>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="product_category_form">
            <field name="name">product_category_form</field>
            <field name="model">product.category</field>
            <field name="inherit_id" ref="product.product_category_form_view"/>
            <field name="priority" eval="16"/>
            <field name="arch" type="xml">
                <field name="type" position="after">
                    <field name="maximal_core_temperature"/>
                    <field name="refusal_temperature"/>
                    <field name="health_approval"/>
                    <field name="dlc_dluo"/>
                    <field name="packaging_type"/>
                </field>
            </field>
        </record>

    </data>
</openerp>