<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record model="ir.actions.server" id="ir_actions_change_move_of_journal">
            <field name="name">Change account move of journal</field>
            <field name="model_id" ref="account.model_account_move"/>
            <field name="code">
action = self.change_move_of_journal(cr, uid, context.get('active_ids'), context)
            </field>
        </record>

        <record model="ir.values" id="ir_value_change_move_of_journal">
            <field name="key">action</field>
            <field name="key2">client_action_multi</field>
            <field name="model">account.move</field>
            <field name="name">Change account move of journal</field>
            <field name="value" eval="'ir.actions.server,'+str(ir_actions_change_move_of_journal)"/>
        </record>

        <record model="ir.ui.view" id="account_change_move_journal_form">
            <field name="name">account_change_move_journal_form</field>
            <field name="model">account.change.move.journal</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="date"/>
                        <field name="journal_id"/>
                    </group>
                    <footer>
                        <button name="execute" string="Change account move of journal" type="object" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</openerp>