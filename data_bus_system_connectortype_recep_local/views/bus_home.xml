<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_type_recep_local_parameter_form" model="ir.ui.view">
            <field name="name">connector.type.recep.local.parameter.form</field>
            <field name="model">connector.type.recep.local.parameter</field>
            <field name="arch" type="xml">
                <form>
                    <group colspan="4" col="4">
                        <field name="path" colspan="2" required="1"/>
                        <field name="file_reg" colspan="2" required="1"/>
                    </group>
                    <footer>
                        <button name="write" string="Sauver" type="object" class="oe_highlight"/>
                        or
                        <button string="Annuler" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</openerp>