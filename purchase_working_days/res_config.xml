<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="purchase_config_form_working_days">
            <field name="name">purchase_config_form_working_days</field>
            <field name="model">purchase.config.settings</field>
            <field name="inherit_id" ref="purchase.view_purchase_configuration"/>
            <field name="priority" eval="16"/>
            <field name="arch" type="xml">
                <data>
                    <form position="inside">
                        <separator string="Default Values"/>
                        <group>
                            <div name="default_values">
                                <div>
                                    <label for="purchase_lead_time"/>
                                    <field name="purchase_lead_time" class="oe_inline"/>
                                </div>
                            </div>
                        </group>
                    </form>
                </data>
            </field>
        </record>

    </data>
</openerp>